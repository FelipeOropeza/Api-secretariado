generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users  {
  id       Int      @id @default(autoincrement())
  name     String
  ra       String  @unique
  password    String
}

model products{
  id       Int      @id @default(autoincrement())
  name     String
  price    Float
  amount     Int @default(0)
}

model productsHistory{
  id Int @id @default(autoincrement())
  productname  String
  typeMovement  String
  amount Int
  username String
  dataMovement DateTime @default(now())
}