generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuarios  {
  id       Int      @id @default(autoincrement())
  nome     String
  ra       String  @unique
  senha    String
}

model produtos{
  id       Int      @id @default(autoincrement())
  nome     String
  preco    Float
  qtd     Int @default(0)
}